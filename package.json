{
  "name": "@kurone-kito/pnpm-project-template",
  "version": "0.1.0",
  "private": true,
  "description": "The boilerplate project of the Node.js",
  "keywords": ["boilerplate", "pnpm", "template"],
  "homepage": "https://github.com/kurone-kito/pnpm-project-template#readme",
  "bugs": "https://github.com/kurone-kito/pnpm-project-template/issues",
  "repository": "github:kurone-kito/pnpm-project-template",
  "license": "MIT",
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "type": "module",
  "files": [],
  "scripts": {
    "clean": "rimraf -g \"*.tgz\" \"*.tsbuildinfo\" node_modules/.cache",
    "lint": "pnpm run \"/lint:.+:check/\"",
    "lint:biome:check": "biome check",
    "lint:biome:fix": "biome check --write",
    "lint:cspell:check": "cspell lint --no-progress --show-suggestions -u \"./**/*\"",
    "lint:fix": "pnpm run lint:cspell:check && pnpm run \"/lint:.+:fix/\"",
    "lint:markdown:check": "markdownlint-cli2 \"**/*.md\"",
    "lint:markdown:fix": "markdownlint-cli2 --fix \"**/*.md\"",
    "prepare": "husky",
    "test": "pnpm run lint"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cspell/cspell-types": "^9.1.1",
    "@kurone-kito/biome-config": "^0.19.0",
    "@kurone-kito/commitlint-config": "^0.19.0",
    "@kurone-kito/cspell-config": "^0.19.0",
    "@kurone-kito/lint-staged-config": "^0.19.0",
    "@kurone-kito/markdownlint-config": "^0.19.0",
    "@kurone-kito/typescript-config": "^0.19.0",
    "cspell": "^9.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "markdownlint-cli2": "^0.18.1",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "engines": {
    "node": "^20.11 || ^22 || >=24"
  },
  "publishConfig": {
    "access": "public"
  }
}
