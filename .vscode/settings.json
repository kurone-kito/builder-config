{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.d.mts": "${capture}.mjs, ${capture}.mjs.*, ${capture}.d.mts.*",
    "*.md": "${capture}.*.md",
    "*.mjs": "${capture}.mjs.*",
    "*.mts": "${capture}.*.mts",
    ".gitattributes": ".gitignore",
    ".markdownlint.*": ".markdownlint-cli2.*",
    ".tool-versions": ".node-version, .nvmrc",
    "package.json": ".npmrc, pnpm-lock.yaml, pnpm-workspace.yaml",
    "tsconfig.json": "*.tsbuildinfo, tsconfig.*.json",
    "typedoc.json": "typedoc.*.json",
    "vite.config.mts": "vitest.config.mts"
  },
  "files.associations": {
    "*.map": "json",
    ".imgbotconfig": "json",
    "LICENSE": "plaintext"
  },
  "files.readonlyInclude": {
    "**/node_modules/**/*": true
  },
  "files.watcherExclude": {
    "**/node_modules/**/*": true
  },
  "git.branchProtection": ["main"],
  "search.exclude": {
    "**/node_modules/**/*": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json": [
      "cspell.config.yml"
    ],
    "https://storage.googleapis.com/coderabbit_public_assets/schema.v2.json": [
      ".coderabbit.yaml"
    ]
  }
}
